token PLUS = "+";
token MINUS = "-";
token ASTERISK = "*";
token SLASH = "/";
token LPAR = "(";
token RPAR = ")";
token NUMBER = /([1-9][0-9]*|0)/;
token WS = /[ \t\n\r]+/;

entry SUM;
prod SUM = FACTOR ((PLUS | MINUS) FACTOR)*;
prod FACTOR = OPERAND ((ASTERISK | SLASH) OPERAND)*;
prod OPERAND = (NUMBER | LPAR SUM RPAR);